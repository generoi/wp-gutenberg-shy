{
  "name": "generoi/wp-gutenberg-shy",
  "type": "wordpress-plugin",
  "license": "MIT",
  "description": "A gutenberg format to insert shy characters for word breaking",
  "homepage": "https://github.com/generoi/wp-gutenberg-shy",
  "authors": [
    {
      "name": "Oskar Schöldström",
      "email": "public@oxy.fi"
    }
  ],
  "keywords": [
    "wordpress",
    "gutenberg"
  ],
  "support": {
    "issues": "https://github.com/generoi/wp-gutenberg-shy/issues"
  },
  "repositories": [],
  "require": {
    "php": ">=7.0.0"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "^3.4.2"
  },
  "autoload": {
    "psr-4": {
      "GeneroWP\\GutenbergShyFormat\\": "src/"
    }
  },
  "scripts": {
    "test": [
      "vendor/bin/phpcs"
    ]
  },
  "archive" : {
    "exclude": [
      ".gitignore"
    ]
  }
}
